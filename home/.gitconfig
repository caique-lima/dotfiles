[user]
  name = Diego Rabatone Oliveira
  email = diraol@diraol.eng.br
	signingkey = 8E521D0F
[alias]
  co = checkout
  ci = commit
  br = branch
  st = status -s
  ps = push
  pl = pull
  ff = merge --ff-only
  lg = log --decorate --graph --pretty=format:\"%Cgreen%h%Creset %s %Cblue(%an)%Creset\"
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  changelog = log --pretty=format:\" * %s\"
  sg = diff --staged
  staged = diff --staged
  nb = checkout -b
  newbranch = checkout -b
  submodule-rm = !git-submodule-rm
  amend = commit --amend
[color]
  ui = auto
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[core]
  whitespace = indent-with-non-tab
  excludesfile = ~/.gitignore
  editor = vim
[github]
  user = diraol
[help]
  autocorrect = 20
[push]
  default = current
[receive]
  denyNonFastForwards = true
[branch]
  autosetuprebase = always
[cola]
  fontdiff = Inconsolata,11,-1,5,50,0,0,0,0,0
[gui]
  editor = gedit
[status]
  submoduleSummary = true
